<resources>

    <style name="Theme.EDS" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#673ab7</item>
        <!--<item name="colorPrimaryVariant">#d1c4e9</item>-->
        <item name="colorPrimaryVariant">#ffffff</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <!-- Secondary brand color. -->
        <!--<item name="colorSecondary">#018786</item>-->
        <item name="colorSecondary">#673ab7</item>
        <item name="colorSecondaryVariant">#03dac5</item>
        <item name="colorOnSecondary">@android:color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#673ab7</item>
        <!-- Custom styles -->
        <item name="android:actionModeBackground">?android:colorAccent</item>
    </style>

    <style name="Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
    </style>
</resources>
